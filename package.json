{
  "name": "fitness-centre",
  "version": "1.0.0",
  "description": "Фитнес-центр + расписание",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.x",
    "autoprefixer": "10.4.x",
    "browser-sync": "2.27.x",
    "eslint": "8.7.0",
    "eslint-config-htmlacademy": "5.0.x",
    "gulp": "4.0.x",
    "gulp-csso": "^4.0.1",
    "gulp-imagemin": "^7.1.0",
    "gulp-plumber": "1.2.x",
    "gulp-postcss": "9.0.x",
    "gulp-posthtml": "^3.0.5",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "3.0.x",
    "gulp-svgstore": "^9.0.0",
    "gulp-webp": "^4.0.1",
